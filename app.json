{
  "expo": {
    "name": "Sky Rabbit Partner",
    "slug": "DeliveryPartner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashandicon.png",
    "splash": {
      "image": "./assets/images/splashandicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50",
      "hideExponentText": true
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashandicon.png",
        "backgroundColor": "#4CAF50"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.avinash_com.DeliveryPartner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
